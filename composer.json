{
  "name": "agenziasmart/swotto",
  "description": "PHP SDK for Swotto API",
  "type": "library",
  "keywords": [
    "swotto",
    "sw4",
    "sdk"
  ],
  "authors": [
    {
      "name": "AgenziaSmart",
      "email": "it@agenziasmart.com",
      "homepage": "http://www.agenziasmart.com/"
    }
  ],
  "require": {
    "php": ">=8.3",
    "guzzlehttp/guzzle": "^7.5",
    "psr/log": "^3.0",
    "psr/http-message": "^1.0 || ^2.0",
    "psr/http-client": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "mockery/mockery": "^1.6",
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpstan/phpstan": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Swotto\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Swotto\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "cs": "php-cs-fixer fix --dry-run --diff",
    "cs-fix": "php-cs-fixer fix --diff --allow-risky=yes",
    "cs-dry": "php-cs-fixer fix --dry-run --diff",
    "phpstan": "phpstan analyse --configuration=phpstan.neon",
    "analyze": "phpstan analyse --configuration=phpstan.neon"
  },
  "minimum-stability": "stable",
  "license": "MIT"
}
